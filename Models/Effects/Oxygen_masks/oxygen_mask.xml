<?xml version="1.0"?>

<PropertyList>

  <path>oxygen_mask.ac</path>

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-property>sim/rendering/static-lod/bare</max-property>
  </animation>

 <animation>
		<type>material</type>
		<object-name>Oxy_mask_1.001</object-name>
		<condition>
		<greater-than-equals>
				<property>/systems/electrical/outputs/efis</property>
				<value>15</value>
		</greater-than-equals>
		<property>/controls/lighting/dome</property>
		</condition>
		<emission>
			<red>0.8</red>
			<green>0.8</green>
			<blue>0.8</blue>
			<factor-prop>/aaa/flightdeck-norm</factor-prop>
		</emission>
	</animation>
  
</PropertyList>
