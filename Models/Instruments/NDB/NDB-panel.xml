	<instrument include="eicas2.xml">
            <condition>
           	 <and>
                <greater-than>
                    <property>systems/electrical/outputs/efis</property>
                    <value>15</value>
                </greater-than>
                <equals>
                    <property>instrumentation/efis/mfd3/display-mode</property>
                    <value>EICAS2</value>
                </equals>
               </and>
            </condition>
        <name>doors</name>
        <x>128</x>
        <y>128</y>
        <w>256</w>
        <h>256</h>
    </instrument>	
	
</instruments>
</PropertyList>