    <speedbrake>
        <name>speedbrake</name>
        <mode>in-transit</mode>
        <path>Sounds/flaps.wav</path>
        <property>surface-positions/speedbrake-norm</property>
        <volume>
            <property>sim/sound/Ovolume</property>
            <max>0.8</max>
            <offset>0.2</offset>
        </volume>
        <position>
            <x>-9</x>
            <y>0</y>
            <z>0</z>
        </position>
        <reference-dist>5.0</reference-dist>
        <max-dist>10.0</max-dist>
    </speedbrake>

    <engines>
        <name>left reverse-thrust</name>
        <mode>looped</mode>
        <path>Sounds/exhaust_loop.wav</path>
        <condition>
			<greater-than>
				<property>engines/engine[0]/rpm</property>
				<value>0</value>
			</greater-than>
			<greater-than>			
				<property>surface-positions/reverser-norm[0]</property>
				<value>0</value>
			</greater-than>			
        </condition>
        <volume>
            <property>sim/sound/Ovolume</property>
        </volume>
        <position>
            <x>2.75</x>
            <y>9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>250.0</reference-dist>
        <max-dist>500.0</max-dist>
    </engines>

    <engines>
        <name>right reverse-thrust</name>
        <mode>looped</mode>
