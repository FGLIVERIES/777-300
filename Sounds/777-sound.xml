<?xml version="1.0"?>

<PropertyList>

<!-- cockpit x=22.5 y=0 z=0.78   -->

<fx>
    <engine>
        <name>left engine</name>
        <path>Aircraft/777-300/Sounds/idle.wav</path>
        <mode>looped</mode>
        <condition>
            <not>
                <property>controls/engines/engine[0]/cutoff</property>
            </not>
        </condition>
        <volume>
            <property>engines/engine[0]/thrust-lbs</property>
            <factor>0.000015</factor>
            <min>0.0</min>
            <max>1</max>
        </volume>
        <volume>
            <property>sim/sound/Ovolume</property>
        </volume>
        <position>
            <x>0</x>
            <y>9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>200.0</reference-dist>
        <max-dist>500.0</max-dist>
    </engine>

    <engine>
        <name>left n1</name>
        <path>Aircraft/777-300/Sounds/whine.wav</path>
        <mode>looped</mode>
        <property>engines/engine[0]/rpm</property>
        <pitch>
            <property>engines/engine[0]/rpm</property>
            <offset>0</offset>
            <factor>0.02</factor>
            <min>0.0</min>
            <max>2.0</max>
        </pitch>
        <volume>
            <property>engines/engine[0]/rpm</property>
            <factor>0.1</factor>
            <min>0.0</min>
            <max>0.6</max>
        </volume>
        <volume>
            <property>sim/sound/Ovolume</property>
        </volume>
        <position>
            <x>0</x>
            <y>9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>30.0</max-dist>
    </engine>

    <engine>
        <name>right engine</name>
        <path>/Aircraft/777-300/Sounds/idle.wav</path>
        <mode>looped</mode>
        <condition>
            <not>
                <property>controls/engines/engine[1]/cutoff</property>
            </not>
        </condition>
        <volume>
            <property>engines/engine[1]/thrust-lbs</property>
            <factor><factor>0.000015</factor></factor>
            <min>0.0</min>
            <max>1.0</max>
        </volume>
        <volume>
            <property>sim/sound/Ovolume</property>
        </volume>
        <position>
            <x>0</x>
            <y>-9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>200.0</reference-dist>
        <max-dist>500.0</max-dist>
    </engine>

    <engine>
        <name>right n1</name>
        <path>Aircraft/777-300/Sounds/whine.wav</path>
        <mode>looped</mode>
        <property>engines/engine[1]/rpm</property>
        <volume>
            <property>engines/engine[1]/rpm</property>
            <factor>0.1</factor>
            <min>0.0</min>
            <max>0.6</max>
        </volume>
        <volume>
            <property>sim/sound/Ovolume</property>
        </volume>
        <pitch>
            <property>engines/engine[1]/rpm</property>
            <offset>0</offset>
            <factor>0.02</factor>
            <min>0.0</min>
            <max>2.0</max>
        </pitch>
        <position>
            <x>0</x>
            <y>-9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>30.0</max-dist>
    </engine>

    <rumblefront>
        <name>nose wheel rumble</name>
        <mode>looped</mode>
        <path>/Aircraft/777-300/Sounds/roll.wav</path>
        <condition>
            <property>gear/gear[0]/wow</property>
        </condition>
        <volume>
            <property>velocities/groundspeed-kt</property>
            <type>log</type>
            <factor>0.25</factor>
        </volume>
        <position>
            <x>-22</x>
            <y>0</y>
            <z>-5</z>
        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>25.0</max-dist>
    </rumblefront>

    <rumblemain>
        <name>main gear rumble</name>
        <mode>looped</mode>
        <path>/Aircraft/777-300/Sounds/roll.wav</path>
        <condition>
            <or>
                <property>gear/gear[1]/wow</property>
                <property>gear/gear[2]/wow</property>
            </or>
        </condition>
        <volume>
            <property>velocities/groundspeed-kt</property>
            <type>log</type>
            <factor>0.25</factor>
        </volume>
        <position>
            <x>-6.5</x>
            <y>0</y>
            <z>-5</z>
        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>25.0</max-dist>
    </rumblemain>

    <gear1>
        <name>gear</name>
        <mode>in-transit</mode>
        <path>Sounds/gear.wav</path>
        <property>gear/gear[0]/position-norm</property>
        <volume>
            <property>sim/sound/Ovolume</property>
            <offset>0.3</offset>
        </volume>
        <pitch>
            <internal>dt_play</internal>
            <offset>0.7</offset>
            <factor>-0.001</factor>
        </pitch>
        <position>
            <x>-22</x>
            <y>0</y>
            <z>-2</z>
        </position>
        <reference-dist>5.0</reference-dist>
        <max-dist>10.0</max-dist>
    </gear1>

    <gearlock>
        <name>gear-lock</name>
		<mode>once</mode>
        <path>Sounds/gear-lck.wav</path>
        <condition>
            <equals>
                <property>gear/gear[0]/position-norm</property>
                <value>0</value>
            </equals>
        </condition>
        <volume>
            <property>sim/sound/Ovolume</property>
            <offset>0.2</offset>
        </volume>
        <position>
            <x>-22</x>
            <y>0</y>
            <z>-2</z>
        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>20.0</max-dist>
    </gearlock>

    <squeal1>
        <name>front wheel squeal</name>
		<mode>once</mode>
        <path>/Aircraft/777-300/Sounds/touch_C.wav</path>
        <condition>
            <property>/gear/gear[0]/wow</property>
        </condition>
        <volume>
            <property>sim/sound/Ovolume</property>
        </volume>
        <volume>
            <property>velocities/airspeed-kt</property>
            <factor>0.01</factor>
        </volume>
        <volume>
            <internal>dt_stop</internal>
            <factor>0.05</factor>
            <max>1.0</max>
        </volume>
        <pitch>
            <property>velocities/airspeed-kt</property>
            <factor>0.0025</factor>
            <offset>1.2</offset>
        </pitch>
        <position>
            <x>-22</x>
            <y>0</y>
            <z>-5</z>
        </position>
        <reference-dist>20.0</reference-dist>
        <max-dist>40.0</max-dist>
    </squeal1>

    <squeal2>
        <name>right wheel squeal</name>
		<mode>once</mode>
        <path>/Aircraft/777-300/Sounds/touch_R.wav</path>
        <condition>
                <property>/gear/gear[1]/wow</property>
        </condition>
        <volume>
            <property>sim/sound/Ovolume</property>
        </volume>
        <volume>
            <property>velocities/speed-down-fps</property>
            <factor>0.05</factor>
        </volume>
        <volume>
            <property>velocities/airspeed-kt</property>
            <factor>0.01</factor>
        </volume>
        <volume>
