            <property>velocities/airspeed-kt</property>
            <factor>0.0025</factor>
            <offset>1.2</offset>
        </pitch>
        <position>
            <x>-6.5</x>
            <y>-8</y>
            <z>-5</z>
        </position>
        <reference-dist>20.0</reference-dist>
        <max-dist>40.0</max-dist>
    </squeal2>

    <squeal3>
        <name>left wheel squeal</name>
		<mode>once</mode>
        <path>/Aircraft/777-300/Sounds/touch_L.wav</path>
