	<eightyknots>
        <name>eightyknots</name>
        <mode>once</mode>
        <type>avionics</type>
        <path>Aircraft/777-300/Sounds/80kt.wav</path>
        <condition>
			<or>
			<property>/gear/gear[0]/wow</property>
			<property>/gear/gear[2]/wow</property>
			</or>	
			<greater-than>
				<property>/velocities/airspeed-kt</property>
